Config {
	font = "xft:zekton-7"
	, bgColor= "#1c1d1f"
	, fgColor= "#b0b0b0"
	, position = Top
	, border = NoBorder
	, borderColor = "#000000"
	, lowerOnStart = True
	, commands = [
		 Run Com "cat" ["/tmp/.conkystats"] "conky" 30
		, Run Date "%a %_d %b %H:%M" "date" 10
		, Run StdinReader
	]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%StdinReader% }{%conky%    %date%"}
