Config {
	font = "xft:zekton-7"
	, bgColor= "#1f1f1b"
	, fgColor= "#9e9c96"
	, position = Top
	, border = NoBorder
	, borderColor = "#000000"
	, lowerOnStart = True
	, commands = [
		 Run Com "cat" ["/tmp/.conkystats"] "conky" 30
		, Run Com "printf" ["%$(echo $(xwininfo -name panel|grep Width|tr -s ' '|cut -d' ' -f3)*0.5|bc)s"] "space" 20
		, Run StdinReader
	]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%StdinReader% }{%conky% %space%"}
