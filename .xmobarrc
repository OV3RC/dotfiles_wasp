Config {
	font = "xft:zekton-7"
	, bgColor= "#1c1d1f"
	, fgColor= "#b0b0b0"
	, position = Top
	, border = NoBorder
	, borderColor = "#000000"
	, lowerOnStart = True
	, commands = [
		 Run Com "cat" ["/tmp/.conkystats"] "conky" 30
		, Run Com "printf" ["%$(echo $(xwininfo -name stalonetray|grep Width|tr -s ' '|cut -d' ' -f3)*0.7|bc)s"] "space" 20
		, Run Date "%_d %b %H:%M" "date" 10
		, Run StdinReader
	]
	, sepChar = "%"
	, alignSep = "}{"
	, template = "%StdinReader% }{%conky% %space% %date%"}
